# https://leetcode.com/problems/next-greater-element-ii/

# O(n) / O(n)
class Solution:
    def nextGreaterElements(self, nums: List[int]) -> List[int]:
        N = len(nums)
        stack = []
        res = [-1] * N
        
        for _ in range(2):
            for i in range(N - 1, -1, -1):
                if stack and stack[-1] <= nums[i]:
                    while stack and stack[-1] <= nums[i]:
                        stack.pop()
                        
                if stack:
                    res[i] = stack[-1]
                else:
                    res[i] = -1

                stack.append(nums[i])

        return res
