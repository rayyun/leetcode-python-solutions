# https://leetcode.com/problems/increasing-triplet-subsequence/

# O(n) / O(1)
class Solution:
    def increasingTriplet(self, nums: List[int]) -> bool:
        first, second = float(inf), float(inf)
        
        for n in nums:
            if n <= first:
                first = n
            elif n <= second:
                second = n
            else:
                return True
            
        return False
