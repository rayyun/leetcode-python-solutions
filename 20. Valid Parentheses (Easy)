# https://leetcode.com/problems/valid-parentheses/description/

# O(n) / O(n) - n: len(s)
class Solution:
    def isValid(self, s: str) -> bool:
        stack = []
        opening = {'(', '[', '{'}
        closing = {')': '(', ']': '[', '}': '{'}

        for c in s:
            if c in opening:
                stack.append(c)
            else:
                if stack and stack[-1] == closing[c]:
                    stack.pop()
                else:
                    return False

        return len(stack) == 0
